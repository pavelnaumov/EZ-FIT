<div class="col-xs-12 col-sm-6 col-lg-4">
  <div class="category-card">
    <div class="category-card-title">
      
      <% @categories.each do |category| %>
      <p> <%= category.name %></p>
      <p> <%= category.number_of_visits %></p>
      <p><%= humanized_money_with_symbol(category.price) %></p> 
      <%#= link_to category.name, gym_category_path(@gym, category.id), class: "btn btn-success" %>

    </div>
    <% end %>
  </div>
</div>